<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="ericsProjects">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ericsProject" minOccurs="1" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Title" type="xs:string"/>
              <xs:element name="Description" type="xs:string"/>
              <xs:element name="TimeToComplete" type="xs:string"/>
              <xs:element name="DatePublished" type="xs:string"/>
              <xs:element name="Link" type="xs:anyURI"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

</xs:schema>